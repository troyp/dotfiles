<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="shift delete" />
  </action>
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt minus" />
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="CIDR.Lang.IntroduceDefine" />
  <action id="CloseActiveTab">
    <keyboard-shortcut first-keystroke="shift ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="shift k" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="alt semicolon" />
  </action>
  <action id="EditMacros">
    <keyboard-shortcut first-keystroke="shift alt 2" second-keystroke="e" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="EditorChooseLookupItemCompleteStatement">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="shift ctrl slash" />
  </action>
  <action id="EditorCompleteStatement">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl slash" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl alt d" />
  </action>
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="alt m" second-keystroke="shift d" />
  </action>
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="alt m" second-keystroke="d" />
  </action>
  <action id="EditorIndentLineOrSelection">
    <keyboard-shortcut first-keystroke="ctrl alt back_slash" />
  </action>
  <action id="EditorLineStart">
    <keyboard-shortcut first-keystroke="home" />
    <keyboard-shortcut first-keystroke="ctrl a" />
  </action>
  <action id="EditorStartNewLineBefore">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
  </action>
  <action id="Find">
    <keyboard-shortcut first-keystroke="alt f3" />
  </action>
  <action id="FindWordAtCaret">
    <keyboard-shortcut first-keystroke="ctrl f3" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="shift 8" />
  </action>
  <action id="FixDocComment">
    <keyboard-shortcut first-keystroke="alt m" second-keystroke="c" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift ctrl i" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="shift ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="shift period" />
  </action>
  <action id="IncrementalSearch">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="s" />
    <keyboard-shortcut first-keystroke="ctrl s" />
  </action>
  <action id="JumpToLastChange">
    <keyboard-shortcut first-keystroke="shift ctrl back_space" />
    <keyboard-shortcut first-keystroke="shift ctrl semicolon" />
  </action>
  <action id="JumpToNextChange">
    <keyboard-shortcut first-keystroke="shift ctrl alt semicolon" />
  </action>
  <action id="MainMenu">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="NextEditorTab">
    <keyboard-shortcut first-keystroke="shift alt right" />
    <keyboard-shortcut first-keystroke="ctrl tab" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="n" />
    <keyboard-shortcut first-keystroke="ctrl tab" />
  </action>
  <action id="PlaySavedMacrosAction">
    <keyboard-shortcut first-keystroke="shift alt 2" second-keystroke="p" />
  </action>
  <action id="PlaybackLastMacro">
    <keyboard-shortcut first-keystroke="shift alt 2" second-keystroke="space" />
  </action>
  <action id="PreviousEditorTab">
    <keyboard-shortcut first-keystroke="shift alt left" />
    <keyboard-shortcut first-keystroke="shift ctrl tab" />
  </action>
  <action id="QuickImplementations">
    <keyboard-shortcut first-keystroke="ctrl q" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="alt button2" />
    <keyboard-shortcut first-keystroke="ctrl h" />
  </action>
  <action id="SafeDelete">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl d" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="slash" />
  </action>
  <action id="StartStopMacroRecording">
    <keyboard-shortcut first-keystroke="shift alt 2" second-keystroke="shift alt 2" />
  </action>
  <action id="StructuralSearchPlugin.StructuralReplaceAction">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="shift r" />
    <keyboard-shortcut first-keystroke="shift ctrl alt 5" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="TypeHierarchy">
    <keyboard-shortcut first-keystroke="ctrl alt 8" />
  </action>
</keymap>